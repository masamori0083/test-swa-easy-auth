{
  "platform": {
    "apiRuntime": "node:18"
  },
	"routes": [
    {
      "route": "/logout",
      "redirect": "/login"
    },
    {
      "route": "/",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/*",
      "statusCode": 200
    }
  ],
	"responseOverrides": {
		"401": {
			"redirect": "/login",
			"statusCode": 302
		}
	},
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  }
}